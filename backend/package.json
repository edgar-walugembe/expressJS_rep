{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "logging": false,
  "scripts": {
    "start": "npm run create:db && npm run run:migrations && npm run start:app",
    "start:app": "node ./bin/www",
    "start:dev": "nodemon --inspect",
    "create:db": "sequelize db:create",
    "run:migrations": "sequelize db:migrate",
    "insert:data": "node ./insert-data.js"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.0",
    "passport-jwt": "^4.0.1",
    "sequelize": "^6.33.0",
    "sequelize-cli": "^6.6.1"
  },
  "nodemonConfig": {
    "ignore": [
      "**/test/**",
      "**/docs/**"
    ],
    "delay": 2500
  }
}
